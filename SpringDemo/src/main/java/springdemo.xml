            <property name = "aid" ref ="a_id"/>
      
   </bean>
   <bean id="a_id" class="com.spring.SpringDemo.Address">
   <property name="pin" value="41313" />
<!--    <property name="city" value="mumbai" />
 -->   </bean>

</beans>